<tr id="studentRow_<%= student.id %>" >
  <td><%= student.name_with_email %></td>
  <td>
    <%= link_to 'View', admin_student_path(student), class: "btn btn-primary btn-sm" %>
    <%= link_to 'Edit', edit_admin_student_path(student), class: "btn btn-warning btn-sm",data: { toggle: 'modal', target: '#editStudentModal', remote: true } %>
    <%= link_to 'Delete', admin_student_path(student), method: :delete, data: { confirm: 'Are you sure?', remote: true }, class: "btn btn-danger btn-sm" %>
  </td>
</tr>
